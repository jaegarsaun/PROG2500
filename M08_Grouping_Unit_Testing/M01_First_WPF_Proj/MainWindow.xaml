<Window x:Class="M01_First_WPF_Proj.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:M01_First_WPF_Proj"
        mc:Ignorable="d"
        Title="MainWindow" Height="850" Width="925">
    <Grid x:Name="myGrid">
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="38,40,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Canvas x:Name="myCanvas" HorizontalAlignment="Left" Height="739" Margin="135,57,0,0" VerticalAlignment="Top" Width="762"/>
        <CheckBox x:Name="checkBox" Content="CheckBox" HorizontalAlignment="Left" Margin="13,88,0,0" VerticalAlignment="Top" Width="100" Checked="checkBox_Checked"/>
        <Button x:Name="doGrouping" Content="Do Grouping" HorizontalAlignment="Left" Margin="38,136,0,0" VerticalAlignment="Top" Width="75" Click="Do_Grouping_Click"/>
        <Button x:Name="makeDots" Content="Make Dots" HorizontalAlignment="Left" Margin="426,21,0,0" VerticalAlignment="Top" Width="75" Click="Make_Dots_Click"/>
    </Grid>
</Window>
